<?xml version="1.0"?>
<transfers
    xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd"
>

    <transfer name="ProductBundle">
        <property name="skuProductConcreteBundle" dataBuilderRule="isbn10" />
        <property name="isNeverOutOfStock" dataBuilderRule="boolean" />
        <property name="availability" dataBuilderRule="numberBetween(10, 999)" />
    </transfer>

    <transfer name="ProductForBundle">
        <property name="quantity" dataBuilderRule="numberBetween(1, 5)" />
        <property name="sku" dataBuilderRule="isbn10" />
        <property name="isActive" dataBuilderRule="boolean" />
    </transfer>

</transfers>
